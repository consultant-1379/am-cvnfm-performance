            <stringProp name="scriptLanguage">java</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">import org.apache.jmeter.services.FileServer;
import org.apache.commons.io.FileUtils;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

Pattern usernamePattern = Pattern.compile(&quot;- name: (.+)&quot;);
Pattern clientCertificateData = Pattern.compile(&quot;client-certificate-data: (.+)&quot;);
Pattern clientKeyData = Pattern.compile(&quot;client-key-data: (.+)&quot;);

String basePath = FileServer.getFileServer().getBaseDir();
String separator = File.separator;
String pathToClusterTemplate = basePath + separator + &quot;${clusterConfigsFolder}&quot;;

File clusterConfigFolder = new File(pathToClusterTemplate);
File clusterConfig = clusterConfigFolder.listFiles()[0];
vars.put(&quot;configPath&quot;, clusterConfig.getAbsolutePath());

String configContent = FileUtils.readFileToString(clusterConfig, &quot;UTF-8&quot;);

Matcher usernameMatcher = usernamePattern.matcher(configContent);
Matcher clientCertificateDataMatcher = clientCertificateData.matcher(configContent);
Matcher clientKeyDataMatcher = clientKeyData.matcher(configContent);
if(usernameMatcher.find()) {
	String username = usernameMatcher.group(1);
	vars.put(&quot;clusterUsername&quot;, username);
}
if(clientCertificateDataMatcher.find()) {
	String certificateData = clientCertificateDataMatcher.group(1);
	vars.put(&quot;client-certificate-data&quot;, certificateData);
}
if(clientKeyDataMatcher.find()) {
	String keyData = clientKeyDataMatcher.group(1);
	vars.put(&quot;client-key-data&quot;, keyData);
}</stringProp>
