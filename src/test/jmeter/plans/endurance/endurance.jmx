              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import org.apache.jmeter.services.FileServer;
import org.apache.commons.io.FilenameUtils;
import org.apache.commons.io.FileUtils;

String basePath = FileServer.getFileServer().getBaseDir();
String separator = File.separator;
String pathToClusterTemplate = basePath + separator + &quot;${clusterConfigsFolder}&quot;;

File clusterConfigFolder = new File(pathToClusterTemplate);
File[] configList = clusterConfigFolder.listFiles();
String configName = FilenameUtils.getBaseName(configList[0].getName());

String generatedName = configName + &quot;-${clusterConfigCounter}.config&quot;;
String generatedPath = &quot;/tmp/&quot; + generatedName;
